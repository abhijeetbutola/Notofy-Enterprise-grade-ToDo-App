{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "google",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "tailwindcss", "@typescript-eslint", "prettier"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "require-jsdoc": "off",
    "prettier/prettier": ["error", {}],
    "@typescript-eslint/no-unused-vars": "off"
    // "@tailwindcss/no-unused-class": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": []
}

